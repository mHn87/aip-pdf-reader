{
  "buildCommand": "npm run build",
  "installCommand": "npm install && pip install -r requirements.txt",
  "functions": {
    "api/**/*.ts": {
      "runtime": "vercel-node@18.x"
    },
    "api/**/*.py": {
      "runtime": "vercel-python@3.11"
    }
  },
  "build": {
    "env": {
      "PYTHON_SETUPTOOLS_ENABLE_FEATURES": "legacy-editable"
    }
  }
}